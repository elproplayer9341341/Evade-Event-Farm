TitleBar.Active = true
TitleBar.Draggable = true

Toggle.MouseButton1Click:Connect(function()
	if not Active1.Value then
		Active1.Value = true
		Toggle.BackgroundColor3 = Color3.fromRGB(55, 255, 24)
		Toggle.ImageTransparency = 0
		Toggle.BackgroundTransparency = 0
while Active1.Value == true do
wait()
local E = game.Workspace["Game"].Effects.Tickets
local tp = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
game.ReplicatedStorage.Events.Respawn:FireServer()
print("Auto Farming..")
for i,v in pairs(E:GetChildren()) do
if v:FindFirstChild("HumanoidRootPart") then
tp.CFrame = v.HumanoidRootPart.CFrame
end
end
end
	else
	if Active1.Value then
		Active1.Value = false
		Toggle.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		Toggle.BackgroundTransparency = 1
		Toggle.ImageTransparency = 1
	end
	end
end)

OpenAndClose.MouseButton1Click:Connect(function()
	if not Home.Visible then
		Home.Visible = true
		OpenAndClose.BackgroundColor3 = Color3.fromRGB(55, 255, 24)
	else
	if Home.Visible then
		Home.Visible = false
		OpenAndClose.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	end
end)

NewButton.MouseButton1Click:Connect(function()
	local GC = getconnections or get_signal_cons
	if GC then
		for i,v in pairs(GC(Players.LocalPlayer.Idled)) do
			if v["Disable"] then
				v["Disable"](v)
			elseif v["Disconnect"] then
				v["Disconnect"](v)
			end
		end
	else
		Players.LocalPlayer.Idled:Connect(function()
			VirtualUser:CaptureController()
			VirtualUser:ClickButton2(Vector2.new())
		end)
	end
	game.StarterGui:SetCore("SendNotification", {
		Title = "Anti Afk",
		Text = "Anti Idle/Afk Enabled",
		Duration = 5
	})
end)
